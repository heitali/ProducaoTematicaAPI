@model IEnumerable<ProducaoTematicaAPI.Models.Sistema>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CO_SISTEMA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SISTEMA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SIGLA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LINK)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CORPORATIVO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MACROPROCESSO)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CO_SISTEMA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SISTEMA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SIGLA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LINK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CORPORATIVO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MACROPROCESSO)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
